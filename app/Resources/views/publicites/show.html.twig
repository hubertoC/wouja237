
{% block body %}
    <h1>Publicite</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ publicite.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ publicite.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ publicite.description }}</td>
            </tr>

        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('publicites_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('publicites_edit', { 'id': publicite.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
